<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="product">
	<select id="productlist" resultType="store.fnfm.vo.ProductVO">
		SELECT pname, filename, category, price
		FROM product
		ORDER BY pname 
	</select>
</mapper>